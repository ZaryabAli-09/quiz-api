{
  "version": 2,
  "builds": [
    {
      "src": "/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/api",
      "dest": "app.js"
    },
    {
      "src": "/api/quiz",
      "dest": "app.js"
    },
    {
      "src": "/api/quiz/questions",
      "dest": "app.js"
    },
    {
      "src": "/api/quiz/questions/get",
      "dest": "app.js"
    },
    {
      "src": "/api/quiz/questions/upload",
      "dest": "app.js"
    }
  ]
}
